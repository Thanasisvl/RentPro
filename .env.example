# Copy this file to ".env" and adjust values as needed.
#
#   cp .env.example .env
#

# Host port for the demo site (nginx serves UI and proxies /api)
WEB_PORT=80

# Backend (FastAPI)
RENTPRO_DATABASE_URL=sqlite:////data/rentpro.db
RENTPRO_UPLOAD_DIR=/data/uploads

# JWT signing secret (set a value; required by docker-compose)
RENTPRO_SECRET_KEY=change-me

# Optional demo seed (1 enables seeding on startup)
RENTPRO_E2E_SEED=0
RENTPRO_E2E_PASSWORD=rentpro-e2e

# Frontend build-time API base URL (for nginx demo use /api)
REACT_APP_API_URL=/api

