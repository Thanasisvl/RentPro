    "test:e2e:stable": "PW_JUNIT=1 CI=1 node scripts/pw.js test --grep-invert @p3",
    "test:e2e:p3:stable": "PW_JUNIT=1 CI=1 node scripts/pw.js test --grep @p3",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
